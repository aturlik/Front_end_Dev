    document.getElementById('LPSR').checked=true;
    dataTopics = dataTopics.replace(' R','').trim();
  };  
  while (dataTopics.includes('Julia')) {
    document.getElementById('LPSJulia').checked=true;
    dataTopics = dataTopics.replace('Julia','').trim();
  };  
  while (dataTopics.includes('Matlab')) {
    document.getElementById('LPSMat').checked=true;
    dataTopics = dataTopics.replace('Matlab','').trim();
  };  
  while (dataTopics.includes('LabVIEW')) {
    document.getElementById('LPSLabv').checked=true;
    dataTopics = dataTopics.replace('LabVIEW','').trim();
  };  
  while (dataTopics.includes('Simulink')) {
    document.getElementById('LPSSimu').checked=true;
    dataTopics = dataTopics.replace('Simulink','').trim();
  };  
  while (dataTopics.includes('SAS')) {
    document.getElementById('LPSSAS').checked=true;
    dataTopics = dataTopics.replace('SAS','').trim();
  };  
  while (dataTopics.includes('COMSOL')) {
    document.getElementById('LPSCOMSOL').checked=true;
    dataTopics = dataTopics.replace('COMSOL','').trim();
  };  
  while (dataTopics.includes('Rhino')) {
    document.getElementById('LPSRhino').checked=true;
    dataTopics = dataTopics.replace('Rhino','').trim();
  };
  
  document.getElementById('DTAOther').value = dataTopics.trim();
  
  <!--Section for other buttons-->
   var SPILstr= "{{{result.[Self Paced vs Instructor Led]}}}".split(',').join(' ');
   if (SPILstr.includes('Self')) {
      document.getElementById('SPILself').checked=true;
   };
   if (SPILstr.includes('Instruct')) {
      document.getElementById('SPILins').checked=true;
   };
   var CDPstr= "{{{result.[Certificate/Degree Program]}}}".split(',').join(' ');
   if (CDPstr.includes('Cert')) {
      document.getElementById('CDPCert').checked=true;
   };
   if (CDPstr.includes('Degree')) {
      document.getElementById('CDPDeg').checked=true;
   };
   var BEstr= "{{{result.[Barrier to Entry]}}}".split(',').join(' ');
   if (BEstr.includes('Beginner')) {
      document.getElementById('BEBeg').checked=true;
   };
  if (BEstr.includes('Intermediate')) {
      document.getElementById('BEInter').checked=true;
   };
  if (BEstr.includes('Advanced')) {
      document.getElementById('BEAdv').checked=true;
   };
   if (BEstr.includes('Expert')) {
      document.getElementById('BEExp').checked=true;
   };
   var PDODstr= "{{{result.[Public/DOD]}}}".split(',').join(' ');
   if (PDODstr.includes('Public')) {
      document.getElementById('PDODP').checked=true;
   };
   if (PDODstr.includes('DOD')) {
      document.getElementById('PDODD').checked=true;
   };
   var PRstr= "{{{result.[In person vs Remote]}}}".split(',').join(' ');
   if (PRstr.includes('Remote')) {
      document.getElementById('PRR').checked=true;
   };
   if (PRstr.includes('Person')) {
      document.getElementById('PRP').checked=true;
   };
   var LTstr= "{{{result.[Learning type]}}}".split(',').join(' ');
   if (LTstr.includes('Text')) {
      document.getElementById('LTT').checked=true;
   };
   if (LTstr.includes('Video')) {
      document.getElementById('LTV').checked=true;
   };
   if (LTstr.includes('Book')) {
      document.getElementById('LTB').checked=true;
   };
   if (LTstr.includes('Lecture')) {
      document.getElementById('LTL').checked=true;
   };
   if (LTstr.includes('Interactive')) {
      document.getElementById('LTI').checked=true;
   };
   
  <!------------------------------>
};
 
 <!--Code to support Darkmode-->
  const darkmode =  new Darkmode();
