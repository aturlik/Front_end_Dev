  while (dataTopics.includes('Computer Skills')) {
    document.getElementById('DTAComSkil').checked=true;
    dataTopics = dataTopics.replace('Computer Skills','').trim();
  };  
  while (dataTopics.includes('Program Skills')) {
    document.getElementById('DTAProSkil').checked=true;
