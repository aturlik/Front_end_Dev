<div class="form-check form-switch" style="float: right;">
    <input class="form-check-input" type="checkbox" id="flexSwitchCheckDefault" align="right" name="radioButton" value="hello"/>
</div>
<script>
  const darkmode =  new Darkmode();
  if(darkmode.isActivated()) {
        $('input[name="radioButton"][value="hello"]').prop('checked', true);
          $('#navbarcolor').removeClass("navbar-light");
          $('#navbarcolor').addClass("navbar-dark");
          $('#modaldark').removeClass("bg-light");
          $('#modaldark').addClass("bg-dark");
      };
      if(!darkmode.isActivated()) {
          $('#navbarcolor').removeClass("navbar-dark");
          $('#navbarcolor').addClass("navbar-light");
          $('#modaldark').removeClass("bg-dark");
          $('#modaldark').addClass("bg-light");
      };
  document.getElementById("flexSwitchCheckDefault").addEventListener("click", function() {
      darkmode.toggle();
      console.log(darkmode.isActivated());
      if(darkmode.isActivated()) {
          $('#navbarcolor').removeClass("navbar-light");
          $('#navbarcolor').addClass("navbar-dark");
          $('#modaldark').removeClass("bg-light");
          $('#modaldark').addClass("bg-dark");
      };
      if(!darkmode.isActivated()) {
          $('#navbarcolor').removeClass("navbar-dark");
          $('#navbarcolor').addClass("navbar-light");
          $('#modaldark').removeClass("bg-dark");
          $('#modaldark').addClass("bg-light");
      };
  });



</script>


<!--===========================================================---
    Styling the results so that they are presented as accordion
    elements
 ---===========================================================-->
<section class="results" id="accordionItems">
    {{# each items }}
        <!--=========================================================---
            Displaying the number of terms that are returned from the
            search
     ---=========================================================-->
        <div>
            {{#if @last}}
                <script>
                    var position = {{@index}}
                    document.getElementById("result").innerHTML = position + 1;
                </script>
            {{/if}}
        </div>
    <div class="item">
        <!--=========================================================---
            Creating the interactable accordion style buttons and the
            headings associated with them
         ---=========================================================-->
        <div class="collapsible" id="collapse{{this._id}}">
            <button class="collapsible" id="collapse_button{{this._id}}" type="button" data-bs-toggle="collapse" data-bs-target="#collapseItemDetails{{this._id}}" aria-expanded="false" aria-controls="collapseItemDetails{{this._id}}">
                <div class="row">
                    <div class="col-sm">
                        <span class="collapsible-heading_pos"> <span class="collapsible-heading"> {{this.Title}} : </span> <span class="collapsible-heading2"> {{ this.[Learning type] }} </span> <span class="tag_style badge bg-secondary" id="Cert/Deg{{this._id}}"> Processing </span></span>
                        <script>
                            var tag =  document.getElementById("Cert/Deg{{this._id}}");
                            if("{{ this.[Certificate/Degree Program] }}" != "No"){
                                tag.innerHTML = "Certificate";
                            }else {
                                tag.remove();
                            }
                        </script>
                    </div>
                    <div class="col-">
                        <i class="arrow up arrow_pos" id="arrow{{this._id}}"></i>
                        <span class="tag_pos tag_style badge">Difficulty: {{ this.[Barrier to Entry]}}</span>
                    </div>
                </div>
                <div class="row">
                    <div class="col-sm comment-syle">
                        {{ this.Comment }}
                    </div>
                    <div class="col-sm">
                        <span class="tag_pos tag_style badge" id="ratingDisp{{this._id}}">Rating: {{ this.[Helpfullness Rating] }}%</span>

                        <script>
                            if("{{ this.[Helpfullness Rating] }}" == ""){
                                document.getElementById("ratingDisp{{this._id}}").innerHTML = "Unrated"
                            }
                        </script>
                    </div>
                </div>
            </button>
            <div>
                <span class="tag_pos2 tag_style badge">{{ this.[In person vs Remote] }}</span>
                <span class="tag_pos3 tag_style badge">{{ this.[Self Paced vs Instructor Led] }}</span>
                <span class="tag_pos4 tag_style badge">{{ this.[Public/DOD] }}</span>
            </div>
            <form action="/get-data/vote" method="post">
                <div class="vote_container">
                    <button type="submit" name="Vote" class="vote_style grow" value="Liked"><i class="fas fa-thumbs-up fa-2x"></i></button>
                    <button type="submit" name="Vote" class="vote_style" value="Disliked"><i class="fas fa-thumbs-down fa-2x"></i></button>

                    <input class="accordion-body" type="text" id="id{{this._id}}" name="id" value="{{this._id}}" property="{{this._id}}">
                    <input class="accordion-body" type="text" id="title{{this._id}}" name="title" value="{{this.Title}}" property="{{this.Titile}}">
                    <input class="accordion-body" type="text" id="url{{this._id}}" name="url" value="hello" property="hello">

                    <script>
                        var id = document.getElementById("id{{this._id}}");
                        var pageURL = document.getElementById("url{{this._id}}");
                        var title = document.getElementById("title{{this._id}}");

                        id.style.display = "none";
                        pageURL.value = window.location.href;
                        pageURL.style.display = "none";
                        title.style.display = "none";
                    </script>
                </div>
            </form>
        </div>

        <script>
            collapse_button{{this._id}}.onfocus = function(){
                if(this.classList.contains("focus_collapsible")){
                    this.classList.remove("focus_collapsible");
                    collapse{{this._id}}.classList.remove("focus_collapsible");
                }else{
                    this.classList.add("focus_collapsible");
                    collapse{{this._id}}.classList.add("focus_collapsible");
                }

                if(arrow{{this._id}}.classList.contains("up")){
                    arrow{{this._id}}.classList.remove("up");
                    arrow{{this._id}}.classList.add("down");
                }else{
                    arrow{{this._id}}.classList.remove("down");
                    arrow{{this._id}}.classList.add("up");
                }
                this.blur();
            }

            collapse_button{{this._id}}.onmouseover = function(){
                this.classList.add("hover_collapsible");
                collapse{{this._id}}.classList.add("hover_collapsible");
            }

            collapse_button{{this._id}}.onmouseout = function(){
                this.classList.remove("hover_collapsible");
                collapse{{this._id}}.classList.remove("hover_collapsible");
            }

        </script>

        <!--=========================================================---
            Creating a container and displaying the extra information
            for each resource
