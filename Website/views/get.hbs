<!-- Styling for each search result -->
<style>

    /*****************************************************************
        Creating the container specifications the contains the results
    *****************************************************************/
    .results {
        margin-top: 25px;
    }

    /*****************************************************************
        Creating the container specifications the contains each
        individual result
    *****************************************************************/


    /* Position */
    .tag_pos {
        float: right;
        /*position: relative;
        right: 10px;
        bottom: 10px;*/
    }

    .tag_pos2 {
        float: left;
        position: relative;
        left: 10px;
    }

    .tag_pos3 {
        float: left;
        position: relative;
        left: 12px;
    }

    .tag_pos4 {
        float: left;
        position: relative;
        left: 14px;
    }

    .tag_pos_rating{
        float: right;
        position: relative;
        right: 10px;
    }

    .collapsible-heading_pos {
        float: left;
        position: relative;
        left: 10px;
    }

    .arrow_pos {
        float: right;
        position: relative;
    }

    .endOfResults{
        text-align: center;
    }

    /* Container Style*/
    .item {
        border-width: 1px;
        border-style: solid;
        border-color: black;
    }

    .vote_style{
        cursor: pointer;
        border: ridge;
        border-color: #718F8A;
    }

    .vote_container{
        text-align: right;
        position: relative;
        right: 15px;
    }

    .collapsible {
        background-color: #eee;
        cursor: pointer;
        width: 100%;
        border: none;
        outline: none;
    }

    .focus_collapsible {
        background-color: #aaa;
    }

    .hover_collapsible {
        background-color: #aaa;
    }

    .navbar-container{
        border-bottom: solid;
        border-bottom-width: 1px
    }

    /* Text Attributes */
    .collapsible-heading {
        font-size: 25px;
        color: #232323;
        font-weight: 500;
    }

    .inner_collapse{
        cursor: pointer;
        width: 25%;
        border: none;
        outline: none;
        padding: 10px
    }

    .collapsible-heading2 {
        font-size: 25px;
        color: #232354;
        font-weight: 200;
    }

    .comment-syle{
        text-align: left;
    }

    .tag_style {
        font-size: 15px;
        background-color: #003042;
    }
    /*****************************************************************
        Creating the specifications or other elements on the page
    *****************************************************************/
    .arrow {
        border: solid black;
        border-width: 0 4px 4px 0;
        display: inline-block;
        padding: 7px;
    }

    .down {
        transform: rotate(45deg);
        -webkit-transform: rotate(45deg);
    }

    .up {
        transform: rotate(-135deg);
        -webkit-transform: rotate(-135deg);
    }

    .pos {
        position: absolute;
        top: 90px;
        right: 20px;
        font-weight: 500;
        font-size: 17px
    }

    .secret{
        visibility: hidden;
    }


